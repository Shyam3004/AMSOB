		script.
			$(document).ready(function(){

				var intSalesInvoiceID = !{JSON.stringify(award[0].intSalesInvoiceID)}

				
				$.post('/bid/awarded', {
					intSalesInvoiceID: intSalesInvoiceID,
				}).then(function(data){
					if(data.indicator){
						awards = data.awards;
						utilities = data.utilities;
						var checkoutbidprice = 0;
						var checkoutbuyerspremium = 0;
						checkouttotal = 0;					
						$('#itemQ').text(awards.length)
						
						for(var i = 0; i < awards.length; i++){
							var award = awards[i];
							console.log(award)
							
							if(award.info.hasOwnProperty('strItemID')){
								award.info.jsonOtherSpecifications = JSON.parse(award.info.jsonOtherSpecifications);
								if(award.info.jsonOtherSpecifications.hasOwnProperty('Screensize'))
									if(!award.info.jsonOtherSpecifications.Screensize.indexOf('Screensize'))
										award.info.jsonOtherSpecifications['Screensize'] = award.info.jsonOtherSpecifications['Screensize']+' inches';						
								if(award.info.jsonOtherSpecifications.hasOwnProperty('Wattage'))
									if(award.info.jsonOtherSpecifications.Wattage.indexOf('watts') < 0)
										award.info.jsonOtherSpecifications['Wattage'] = award.info.jsonOtherSpecifications['Wattage']+' watts';
								if(award.info.jsonOtherSpecifications.hasOwnProperty('Storage') && award.info.jsonOtherSpecifications.category == "Mobile")
									if(!award.info.jsonOtherSpecifications.Storage.indexOf('Storage'))
										award.info.jsonOtherSpecifications['Storage'] = award.info.jsonOtherSpecifications['Storage']+' GB';
								if(award.info.jsonOtherSpecifications.hasOwnProperty('Ram'))
									if(!award.info.jsonOtherSpecifications.Ram.indexOf('RAM'))
										award.info.jsonOtherSpecifications['Ram'] = 'RAM '+award.info.jsonOtherSpecifications['Ram']+' GB';
								var valuesOnly = Object.values(award.info.jsonOtherSpecifications)//array values
								var desc = award.info.jsonOtherSpecifications.Item+' ' 
								for(var l = 1; l < valuesOnly.length; l++){
									desc = desc+' '+valuesOnly[l]
								}
								var percentBuyersPremium = (data.utilities.dblBuyersPremium).toFixed(2);
								var buyersPremium = (award.dblBidPrice * percentBuyersPremium).toFixed(2);
								var rowAward = $(`<tr style="font-weight: bold">
													<td>${award.intSIDQty}</div>												
													<td><img src="/assets/uploads/${award.info.strPicture}" width="100px"/></td>												
													<td>${desc}</td>
													<td class='bidPrice' id="bidPrice${award.info.strItemID}" value="${(award.dblBidPrice).toFixed(2)}">${(award.dblBidPrice).toFixed(2) * award.intSIDQty}</td>
													<td class='buyersPremium' id="buyersPremium${award.info.strItemID}" value="${(buyersPremium)}">${(buyersPremium) * award.intSIDQty}</td>
													<td class='total' id="total${award.info.strItemID}" value="${(parseFloat(award.dblBidPrice) + parseFloat(buyersPremium)).toFixed(2)}">${((parseFloat(award.dblBidPrice) + parseFloat(buyersPremium)).toFixed(2)) * award.intSIDQty}</td>
												</tr>`)
								$('.awardTable').append(rowAward);
								
							}
							if(award.info.hasOwnProperty('intBundleID')){


								var percentBuyersPremium = (data.utilities.dblBuyersPremium).toFixed(2);
								var buyersPremium = (award.dblBidPrice * percentBuyersPremium).toFixed(2);
								var rowAward = $(`<tr>
													<td class="align-center">1</div>
													<td><img src="/assets/uploads/${award.info.strBundlePicture}" width="100px"/></td>
													<td>${award.info.strBundleTitle}</td>
													<td class='bidPrice' id="bidPrice${award.info.intBundleID}" value="${(award.dblBidPrice).toFixed(2)}">${(award.dblBidPrice).toFixed(2)}</td>
													<td class='buyersPremium' id="buyersPremium${award.info.intBundleID}" value="${(buyersPremium)}">${(buyersPremium)}</td>
													<td class='total' id="total${award.info.intBundleID}" value="${(parseFloat(award.dblBidPrice) + parseFloat(buyersPremium)).toFixed(2)}">${(parseFloat(award.dblBidPrice) + parseFloat(buyersPremium)).toFixed(2)}</td>
												</tr>`)
								$('.awardTable').append(rowAward);	
							}	
									


							if(i==awards.length-1){
								$('.bidPrice').map(function(index, e){
									checkoutbidprice += parseFloat($(this).text())
									if(index == $('.bidPrice').length - 1){
										$('#receiptHammerPrice').text('‚Ç±'+(checkoutbidprice).toFixed(2))								

									}
								})

								$('.buyersPremium').map(function(index, e){
									checkoutbuyerspremium += parseFloat($(this).text())
									if(index == $('.buyersPremium').length - 1){
										$('#receiptBuyersPremium').text('‚Ç±'+(checkoutbuyerspremium).toFixed(2))

									}
								})

								$('.total').map(function(index, e){
									checkouttotal += parseFloat($(this).text())
									if(index == $('.total').length - 1){
										
										$('#total').val('‚Ç±'+(checkouttotal).toFixed(2)/2)

									}
								})
							}
							
						}
					}
				})

				$(document).on('click', '#unpaidButton', function(){
					$.post('/unpaid', {
						intSalesInvoiceID: intSalesInvoiceID,
					}).then(function(data){
						if(data.indicator){
							swal({
								title: 'Success',
								text: 'Marked as unpaid successfully.',
								button: 'Okay',
								icon: 'success',								
							})
							$('#receiptField').fadeOut(1000);
							$('#statusPill').text('‚Ä¢ Unpaid');
							$('#statusPill').addClass('badge-danger').removeClass('badge-success').removeClass('badge-warning');
							$('#paidButton').parents('li').fadeIn(1000)
							$('#invalidCredentialButton').parents('li').fadeIn(1000)
							$('#unpaidButton').parents('li').fadeOut(1000)
							$('.receiptID').fadeOut(100)
							
						}

					})
				})

				$(document).on('click', '#invalidCredentialButton', function(){
					$.post('/invalidcredential', {
						intSalesInvoiceID: intSalesInvoiceID,
					}).then(function(data){
						if(data.indicator){
							swal({
								title: 'Success',
								text: 'Marked as invalid credential successfully.',
								button: 'Okay',
								icon: 'success',								
							})
							$('#receiptField').fadeOut(1000);
							$('#statusPill').text('‚Ä¢ Unpaid');
							$('#statusPill').addClass('badge-warning').removeClass('badge-danger').removeClass('badge-success');
							$('#paidButton').parents('li').fadeOut(1000)
							$('#invalidCredentialButton').parents('li').fadeOut(1000)
							$('#unpaidButton').parents('li').fadeOut(1000)
							$('.receiptID').fadeOut(100)
							
						}

					})
				})

				$(document).on('click', '#paidButton', function(){
					$.post('/paid',{
						intSalesInvoiceID: intSalesInvoiceID,
					}).then(function(data){
						if(data.indicator){
							swal({
								title: 'Success',
								text: 'Marked as paid successfully.',
								button: 'Okay',
								icon: 'success',
							})
							$('#receiptField').fadeIn(1000);
							$('#statusPill').text('‚Ä¢ Paid');
							$('#statusPill').addClass('badge-success').removeClass('badge-danger').removeClass('badge-warning');
							$('#paidButton').parents('li').fadeOut(1000)
							$('#invalidCredentialButton').parents('li').fadeOut(1000)
							$('#unpaidButton').parents('li').fadeIn(1000)
							$('.receiptID').fadeIn(100)
						}
					})
				})

				$(document).on('change', '#basicSelect', function(){
					var option = $('#basicSelect option:selected').val();

					if(option == 1){
						$('#comment').val('Thank you for bidding! We are now processing your awards to be delivered.');
					}
					else if(option == 2){
						$('#comment').val('Your award has been shipped. Please expect a call from courier.');
					}
					else if(option == 3){
						$('#comment').val('your award has been delivered. Thank you for bidding with Auction Pride of the Philippines! Bid again!');
					}
				})

				$(document).on('click', '#save', function(){

					if($("#basicSelect option:selected").val() == ""){
						swal({
							title: 'Please Select Status',
							icon: 'error',
						})
					}
					else{
						swal({
							title: 'Are you sure?',
							text: 'Once delivered, the status cannot be reversed.',
							icon: 'warning',
							buttons: {
								Yes: 'Yes',
								No: 'No',
							}
						}).then((value)=>{
							if(value == 'Yes'){
								$.post('/updatestatus',{
									intSalesInvoiceID: intSalesInvoiceID,
									status: $("#basicSelect option:selected").val(),
									statuscomment: $('#comment').val()
								}).done(function(data){
									if(data.indicator){
										location.href = `/award/detail/${intSalesInvoiceID}`
									}
								})
							}
						})
					}
				})


			})