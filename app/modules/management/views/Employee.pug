									li.breadcrumb-item
										a(href='#') Auction Management
									li.breadcrumb-item.active
										| Employee
				.content-body
					.row
						#recent-transactions.col-12
							.card
								.card-header
									h4.card-title Employee
									a.heading-elements-toggle
										i.la.la-ellipsis-h.font-medium-3
									.heading-elements
										button.btn.btn-block.btn-info.btn-sm(type='button', data-toggle='modal', data-target='#signUpModal')
											i.ft-plus.white
											|  Add Employee
								.card-content
									ul.list-inline.mb-0
										.table-responsive
											table#employeeTable.table.table-hover.table-xl.mb-0
												thead
													tr
														th.border-top-0 Name
														th.border-top-0 Job Type
														th.border-top-0 Contact
														th.border-top-0 Action
												tbody
													if employeeList && employeeList.length > 0
														each employee in employeeList
															if(employee.intEmployeeID == 1)
																-continue;	
															tr
																td.text-truncate
																	span #{employee.strEmployeeFirstName}
																td.text-truncate
																	span #{employee.strJobTitle}
																td.text-truncate
																	span #{employee.strContactNumber}
																td
																	span.dropdown
																		button#btnSearchDrop2.btn.btn-info.dropdown-toggle.dropdown-menu-right(type='button', data-toggle='dropdown', aria-haspopup='true', aria-expanded='true')
																			i.ft-settings
																		span.dropdown-menu.mt-1.dropdown-menu-right(aria-labelledby='btnSearchDrop2')
																			a.delete.dropdown-item(href='javascript:void(0)' value=`${employee.intEmployeeID}`)
																				i.icon-pencil
																				|  Delete Bidder
																			a.edit.dropdown-item(data-toggle='modal' data-target='#editModal' value=`${employee.intEmployeeID}`	)
																				i.ft-eye
																				|  Edit
																				//- Modal
												tfoot
														tr
															th.border-top-0 Name
															th.border-top-0 Job Type
															th.border-top-0 Contact
															th.border-top-0 Action
																	//- Sign Up Live Bidder
