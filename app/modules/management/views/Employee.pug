		script.
			function validateForm(){
				$('#AddEmployee').formValidation({
					framework: 'bootstrap',
					icon: {
						valid: 'fa fa-check',
						invalid: 'fa fa-times',
						validating: 'fa fa-spinner'
					},
					fields: {
						firstName: {
							validators: {
								notEmpty: {
									message: `Please Enter Bidder's First Name.`
								}
							}
						},
						lastName: {
							validators: {
								notEmpty: {
									message: `Please Enter Bidder's Last Name.`
								}
							}
						},
						Email: {
							validators: {
								notEmpty: {
									message: `Please Enter Employee's Email Address.`
								}
							}
						},
						Username: {
							validators: {
								notEmpty: {
									message: `Please Enter Employee's Username.`
								}
							}
						},
						Password: {
							validators: {
								notEmpty: {
									message: `Please Enter Employee's Username.`
								}
							}
						},
						Contact: {
							validators: {
								notEmpty: {
									message: `Enter Employee's Phone Number.`,
								},
							}
						},
					}
				})
				.on('err.field.fv', function(e, data) {
					// Get the first invalid field
					var invalidFields = data.fv.getInvalidFields().length;
					$('#errChecker').val(invalidFields)
				})
				.on('success.field.fv', function(e, data) {
					// Get the first invalid field
					var invalidFields = data.fv.getInvalidFields().length;
					$('#errChecker').val(invalidFields)
				});
			}
