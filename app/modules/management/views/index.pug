										legend:{
											display:!1,
											position:"bottom"
										},
										hover:{
											mode:"label"
										},
										scales:{
											xAxes:[{
												display:!1
											}],
											yAxes:[{
												display:!1,
												ticks:{
													min:0,
													max:Math.max.apply(Math, revenues) + Math.max.apply(Math, revenues) / 5
												}
											}]
										},
										title:{
											display:!1,
											fontColor:"#FFF",
											fullWidth:!1,
											fontSize:40,
											text:"82%"
										}
									},
									data:{
										labels: months,
										datasets:[{
											label:"Revenue",
											data: revenues,
