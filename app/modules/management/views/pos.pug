		script.
			$(document).ready(function () {
				$('#bidderTable').dataTable();
				$(document).on('click', '.viewBidder', function(){
					var auctionId = $(this).attr('auctionId');
					$.post('/pos/livebidder', {id: auctionId})
					.done(function(data){
						$('#bidderModalBody').append(`
							<div style="padding-top: 1.5rem; padding-bottom: 0rem;" class="card-header border-0">
								<h6 id="bidderModalTitle" style="margin: -38px 0 20px;" class="card-subtitle line-on-side text-muted text-center font-small-3 pt-2">
									<span>Auction #${auctionId}</span>
								</h6>
							</div>`
						)
						if(data.length>0){
							$('#bidderModalBody').append(`<div class="table-responsive">
								<table class="table table-hover table-xl mb-0">
									<thead>
									<tr>
										<th class="border-top-0">Bidder Number</th>
										<th class="border-top-0">Bidder Name</th>
										<th class="border-top-0">Contact Number</th>
									</tr>
									</thead>
									<tbody id='bidderModalTableBody'></tbody>
								</table>
								</div>`
							)
							for(var i = 0; i<data.length; i++){
								$('#bidderModalTableBody').append(`<tr>
									<td>${data[i].strBidderNumber}</td>
									<td>${data[i].strName}</td>
									<td>${data[i].strContact}</td>
									</tr>`
								)
							}
						}
						else{
							$('#bidderModalBody').append(`<center>
									<h2>No Bidders in this Auction</h2>
								</center>`
							)
						}
						$('#bidderModal').modal('show');
					})
				})
				$('#bidderModal').on('hidden.bs.modal', function() {
					$('#bidderModalBody').empty();
				})
			})


			
