																-if(item.jsonOtherSpecifications.wattage.indexOf('watts') < 0)
																	-item.jsonOtherSpecifications['wattage'] = item.jsonOtherSpecifications['wattage']+' watts';
															-if(item.jsonOtherSpecifications.hasOwnProperty('storage') && item.jsonOtherSpecifications.category == "Mobile")
																-if(!item.jsonOtherSpecifications.storage.indexOf('storage'))
																	-item.jsonOtherSpecifications['storage'] = item.jsonOtherSpecifications['storage']+' GB';
															-if(item.jsonOtherSpecifications.hasOwnProperty('ram'))
																-if(!item.jsonOtherSpecifications.ram.indexOf('RAM'))
																	-item.jsonOtherSpecifications['ram'] = 'RAM '+item.jsonOtherSpecifications['ram']+' GB';
															-var valuesOnly = Object.values(item.jsonOtherSpecifications)//-array values
															-console.log(valuesOnly)
															-var desc = item.jsonOtherSpecifications.Item+' ' 
															-for(var x = 1; x < valuesOnly.length; x++)
																-if(!(Object.keys(item.jsonOtherSpecifications)[x] == "UOM"))
																	-if(!(Object.keys(item.jsonOtherSpecifications)[x] == "price")) 
																		-if(!(Object.keys(item.jsonOtherSpecifications)[x] == "quantity")) 
